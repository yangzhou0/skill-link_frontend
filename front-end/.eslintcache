 HTTP Request\n    // call API \n  }\n\n  const handleKeyDown = (e) => {\n    if (e.keyCode === 13) {\n      setActiveOption(0)\n      setShowOptions(false)\n      setInput(autoCompleteResults[activeOption])\n    } else if (e.keyCode === 38) {\n      if (activeOption === 0) {\n        return;\n      }\n      setActiveOption(activeOption - 1)\n    } else if (e.keyCode === 40) {\n      if (activeOption === autoCompleteResults.length - 1) {\n        return;\n      }\n      setActiveOption(activeOption + 1)\n    }\n  }\n\n  return (\n    <React.Fragment>\n      <div className = 'search'>\n        <input className=\"search-box\" onChange={(e) =>handleType(e)} onKeyDown={(e)=>{handleKeyDown(e)}} type = 'text' value = {input}/>\n        <input type=\"submit\" value ='' className = 'search-btn' onClick = {(e)=>handleSearch(e)} />\n        {/* {searchContent && <JobOverview jobTitle ={searchContent} />} */}\n      </div>\n      <ul className=\"options\">\n        {showOptions && input && autoCompleteResults.map((option,index) =>(\n          <li className = {index === activeOption ? 'option-active' : ''} onClick={e=>handleClick(e)}>{option}</li>\n        ))}\n      </ul>\n    </React.Fragment>\n  )\n}\n",{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":1,"column":8,"nodeType":"71","messageId":"72","endLine":1,"endColumn":12},{"ruleId":"69","severity":1,"message":"73","line":3,"column":10,"nodeType":"71","messageId":"72","endLine":3,"endColumn":14},{"ruleId":"69","severity":1,"message":"73","line":3,"column":10,"nodeType":"71","messageId":"72","endLine":3,"endColumn":14},{"ruleId":"69","severity":1,"message":"74","line":1,"column":25,"nodeType":"71","messageId":"72","endLine":1,"endColumn":34},{"ruleId":"69","severity":1,"message":"75","line":6,"column":9,"nodeType":"71","messageId":"72","endLine":6,"endColumn":21},{"ruleId":"69","severity":1,"message":"76","line":1,"column":15,"nodeType":"71","messageId":"72